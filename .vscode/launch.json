    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["--with_tracking",
                "--run_name", "main run",
                "--start", "0",
                "--end", "100",
                "--decoder_inv_steps", "200",
                "--decoder_lr", "0.05",
                "--inversion_type", "new_alg",
                // "--inversion_type", "dec_inv",
                // "--inversion_type", "ete",
                // "--use_float",
                "--test_beta",
                "--forward_steps", "1",
                "--tuning_steps", "0",
                "--reg_coeff", "0.0"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}